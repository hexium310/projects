{
  "name": "projects",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production && yarn copy:markdowns && yarn copy:images",
    "watch": "webpack serve --mode development",
    "start": "yarn watch",
    "copy:markdowns": "mkdir -p dist/contents && cp contents/*.md dist/contents/",
    "copy:images": "mkdir -p dist/contents/images && cp contents/images/* dist/contents/images/",
    "lint": "eslint '**/*.{js,ts,tsx}' --ignore-path .gitignore"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "custom-dashed-border": "^0.1.1",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.24.0",
    "html-webpack-plugin": "^5.3.1",
    "js-yaml-loader": "^1.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.2",
    "react-router-dom": "^5.2.0",
    "source-map-loader": "^3.0.0",
    "styled-components": "^5.3.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
