{
  "name": "projects",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production && yarn copy:markdowns && yarn copy:images",
    "watch": "webpack-dev-server --mode development",
    "start": "yarn watch",
    "copy:markdowns": "mkdir -p dist/contents && cp contents/*.md dist/contents/",
    "copy:images": "mkdir -p dist/contents/images && cp contents/images/* dist/contents/images/",
    "lint": "eslint '**/*.{js,ts,tsx}' --ignore-path .gitignore"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^4.4.3",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "custom-dashed-border": "^0.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "html-webpack-plugin": "^3.2.0",
    "js-yaml-loader": "^1.2.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.1.2",
    "source-map-loader": "^0.2.4",
    "styled-components": "^5.0.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
