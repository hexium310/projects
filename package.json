{
  "name": "projects",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production && yarn copy:markdowns && yarn copy:images",
    "watch": "webpack-dev-server --mode development",
    "start": "yarn watch",
    "copy:markdowns": "mkdir -p dist/contents && cp contents/*.md dist/contents/",
    "copy:images": "mkdir -p dist/contents/images && cp contents/images/* dist/contents/images/",
    "lint": "eslint '**/*.{js,ts,tsx}' --ignore-path .gitignore"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "custom-dashed-border": "^0.1.1",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "html-webpack-plugin": "^4.3.0",
    "js-yaml-loader": "^1.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.2.0",
    "source-map-loader": "^1.0.1",
    "styled-components": "^5.1.1",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
